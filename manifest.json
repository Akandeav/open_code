{
    "manifest_version": 3,
    "author": "akandevic@gmail.com",
    "name": "Open Code",
    "version": "1.0.0",
    "description": "Open any active github repository in vscode web",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_popup": "popups/popup.html"
    },
    "host_permissions": [
        "https://github.com/*"
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}